<?xml version="1.0" encoding="UTF-8"?>
<messages>
	<message id="header-user">
			<label>${logged-in-user}</label>
	</message>

	<message id="start-form">
		${header}
		<div class="start-form container">
			<div class="row">
				<div class="span6">
					<div class="form-horizontal">
					  <div class="form-group">
					    <div class="col-sm-10">
					      ${beer-search-text}
					    </div>
					  </div>
		
					  <div class="form-group">
					    <div class="col-sm-offset-2 col-sm-10">
					     	${beer-search-button}
					    </div>
					  </div>
					</div>
				</div>
				<div class="span6">
					<div class="form-horizontal">
					  <div class="form-group">
					    <div class="col-sm-10">
					      ${bar-search-text}
					    </div>
					  </div>
		
					  <div class="form-group">
					    <div class="col-sm-offset-2 col-sm-10">
					     	${bar-search-button}
					    </div>
					  </div>
					</div>
				</div>
			</div>

			<div class="row">
				${login}
			</div>
		</div>
	</message>
	
	<message id="beer-form">
		${header}
		
		
		<div class="div-row">
			<table>
				<tr>
					<td>
						<div>${beer}</div>
						<div>${tr:beer-form.brewed-by}: ${brewery}</div>
						<div>${tr:beer-form.alcohol}: ${alcohol} Â°</div>
						<div>${tr:beer-form.favored-by}: ${favored-by}</div>
						<div>${tr:beer-form.score}: ${score} / ${highest-score}</div>
					</td>
					<td>
						<img src="${picture-url}" />
					</td>
				</tr>
			</table>	
		</div>
		<div class="div-row">
			<span>TODO: facebook like</span>
			<span>TODO: facebook share</span>
			${add-to-favorites}
		</div>
		<a target="_blank" href="${find-bars-url}">${find-bars-text}</a>
		<div class="div-row">
			<table>
				<tr>
					<td>${tr:beer-form.tags}:</td> <td>${tags}</td>
				</tr>
				<tr>
					<td></td> <td>${add-tag}</td>
				</tr>
			</table>
		</div>
		<div class="div-row">
			<div>${tr:beer-form.reviews}:</div>
			${add-review}
			${reviews}
		</div>
	</message>
	
	<message id="tag">
		<div class="tag">${text}</div>
	</message>
	
	<message id="review">
		<div class="review">
			<div class="div-row">
				<div class="average-score">${tr:review.average-score}:${average-score}</div>
				<div class="poster">${tr:review.posted-by}: ${poster}</div>
			</div>
			<div class="div-row">
				<div class="score">${tr:review.color}:${color-score}</div>
				<div class="score">${tr:review.smell}:${smell-score}</div>
				<div class="score">${tr:review.taste}:${taste-score}</div>
				<div class="score">${tr:review.feel}:${feel-score}</div>
			</div>
			<div class="div-row">${text}</div>
			<div class="div-row">${control}</div>
			<div class="div-row">${comments}</div>
		</div>
	</message>
	
	<message id="ellipsis">
		<span>...</span>
	</message>
	

	<message id="add-tag-dialog">
		<div class="dialog">
			<div class="div-row">${type-combo}</div>
			<div class="div-row">${variable-field}</div>
			<div class="controls">${ok} ${cancel}</div>
		</div>
	</message>
	
	<message id="add-review-dialog">
			  <span class="control-group">
			    <label class="control-label" for="${id:color}">
			      ${tr:add-review-dialog.color}
			    </label>
			    <span class="controls">
			      ${color}
			    </span>
			  </span>
			  
			  <span class="control-group">
			    <label class="control-label" for="${id:smell}">
			      ${tr:add-review-dialog.smell}
			    </label>
			    <span class="controls">
			      ${smell}
			    </span>
			  </span>
			    
			  <span class="control-group">
			    <label class="control-label" for="${id:taste}">
			      ${tr:add-review-dialog.taste}
			    </label>
			    <span class="controls">
			      ${taste}
			    </span>	
			  </span>  
			  
			  <span class="control-group">
			    <label class="control-label" for="${id:feel}">
			      ${tr:add-review-dialog.feel}
			    </label>
			    <span class="controls">
			      ${feel}
			    </span>
			  </span>
			  
			  <span class="control-group">
			    <label class="control-label" for="${id:text}">
			      ${tr:add-review-dialog.text}
			    </label>
			    <span class="controls">
			      ${text}
			    </span>
			  </span>
			
			<div class="form-actions">
				<div class="controls">${ok} ${cancel}</div>
			</div>		
	</message>

	<message id="user-form">
		${header}
		
	
		<div class="personal-info">
			<img src="${user-pic-url}" class="span3 offset3"/>
			<div class="span6">
			<div class="name row-fluid"><div>${first-name} ${last-name}</div>
			</div>
			
			<div class="row-fluid"><div>${is-drinking-in}</div></div>
			<div class="row-fluid"><div>${share-location}</div></div>
			</div>
			
		</div>
		${favorites}
		<a target="_blank" href="${app.base.url}/${find-nearby-friends-url}">${tr:user-form.find-nearby-friends}</a>
		<a target="_blank" href="${app.base.url}/${find-nearby-bars-url}">${tr:user-form.find-nearby-bars}</a>
	</message>
	
	<message id="is-drinking-in">
		<span class="is-drinking-in">${text}</span>
	</message>
	
	<message id="user-favorites">
		<table class="favorites table table-striped">
			<tr>
				<td colspan="2">${tr:user-form.favorite-beers}</td>
				<td colspan="2">${tr:user-form.favorite-bars}</td>
			</tr>
			<tr>
				<td>${fav-beer-1}</td>
				<td>${fav-beer-delete-1}</td>
				<td>${fav-bar-1}</td>
				<td>${fav-bar-delete-1}</td>
			</tr>
			<tr>
				<td>${fav-beer-2}</td>
				<td>${fav-beer-delete-2}</td>
				<td>${fav-bar-2}</td>
				<td>${fav-bar-delete-2}</td>
			</tr>
			<tr>
				<td>${fav-beer-3}</td>
				<td>${fav-beer-delete-3}</td>
				<td>${fav-bar-3}</td>
				<td>${fav-bar-delete-3}</td>
			</tr>
			<tr>
				<td>${fav-beer-4}</td>
				<td>${fav-beer-delete-4}</td>
				<td>${fav-bar-4}</td>
				<td>${fav-bar-delete-4}</td>
			</tr>
			<tr>
				<td>${fav-beer-5}</td>
				<td>${fav-beer-delete-5}</td>
				<td>${fav-bar-5}</td>
				<td>${fav-bar-delete-5}</td>
			</tr>
		</table>
	</message>
	
	<message id="user-favorite-link">
		<a href="${app.base.url}/${url}">${name}</a>
	</message>
		
	<message id="comments-widget">
		<span>${tr:comments.all-comments} (${number})</span>
		${add-comment}
		${comments}
	</message>
	
	<message id="add-comment-widget">
		<div class="div-row">
			<div class="span1"><img src="${poster-img}"/></div>
			<div>
			
			<div class="row-fluid">
				${text}
				<break/>
				${cancel} ${post}
			</div>
			</div>
		</div>
	</message>
	
	<message id="comment-widget">
		<div class="span1"><img src="${poster-img}"/></div>
		<div>
			
			<div class="row-fluid">
			
				<span>${tr:comment.posted-by}:${poster-name}</span> <span>${posted-x-time-ago}</span>
				<break/>
			</div>
			<div class="row-fluid offset1">${text}</div>
		</div>
	</message>
	
	<message id="bar-form">
		${header}	
		<div class="row-fluid">
			<h1 id="bar-title" >${bar}</h1>
		</div>
		<div class="row-fluid">
			<div class="span4">${picture}
			<div class="div-row">
		   <object type="text/html" data="${facebook}"></object>
			<div class="controls">${add-to-favorites}</div>
		   </div>
			</div>
						
					<div class="span4">
						<div class="row-fluid">
						<div class="span4">${tr:bar-form.website}:</div><div class="span8"> ${website}</div>
						</div>
						<div class="row-fluid">
						<div class="span4">${tr:bar-form.address}:</div><div class="span8"> ${address1}</div>
						</div>
						<div class="row-fluid">
						<div class="span8 offset4"> ${address2}</div>
						</div>
						<div class="row-fluid">
						<div class="span8 offset4"> ${address3}</div>
						</div>
						<div class="row-fluid">
						<div class="span4">${tr:bar-form.favored-by}:</div><div class="span8"> ${favored-by}</div>
						</div>
						<div class="row-fluid">
						<div class="span4">${tr:bar-form.score}:</div><div class="span8"> ${score}/10 ${change-score}</div>
						</div>
						<div class="row-fluid">
						<div>${score-chart}</div>
						</div>
					</div>
					<div class="span4">
						<div>${tr:bar-form.beerlist-tile}</div>
						${beer-list}	
					</div>	
		</div>
		
		<div>${comments}</div>
	</message>
	
	<message id="beer-list-item">
	<div>${name}</div>
	</message>
	
	
	<message id="add-beer-dialog">
	<div class="dialog">
	<form class="form">
			<div >${field}</div>
			<div class="controls">${add-beer} ${cancel}</div>
		</form>	
	</div>
	</message>
	
	<message id="new-bar-form">	
		<form class="form-horizontal">
			<div>
				<div class="control-group">
				<label class="control-label" for="inputName">${tr:new-bar-form.name}:</label> ${bar}
				</div>
				<div class="control-group">
				<label class="control-label" for="inputWebsite">${tr:new-bar-form.website}:</label> ${website}
				</div>
				<div class="control-group">
				<label class="control-label" for="inputStreet">${tr:new-bar-form.street}:</label> ${street} 
				</div>
				<div class="control-group">
				<label class="control-label" for="inputNumber">${tr:new-bar-form.number}:</label> ${number} 
				</div>
				<div class="control-group">
				<label class="control-label" for="inputZipcode">${tr:new-bar-form.zipcode}:</label> ${zipcode} 
				</div>
				<div class="control-group">
				<label class="control-label" for="inputCity">${tr:new-bar-form.city}:</label> ${city}
				</div>
				<div class="control-group">
				<label class="control-label" for="inputCountry">${tr:new-bar-form.country}:</label> ${country}
				</div>
				<div class="controls">${ok} ${cancel}</div>
			</div>
			
		</form>
	</message>
	
	<message id="beer-search-form">
	<div class="center">
		${header}
		
		<h2>${tr:beer-search.select-beer}</h2>
		<div class="input-append">${field} ${find}</div>
		${results}
		</div>
	</message>
	
	<message id="beer-search-results">
		<h3>${tr:beer-search.results}</h3>
		<div class="result-entries">${result-entries}</div>
	</message>
	
	<message id="beer-search-result">
		<a href="${app.base.url}/${relative-url}">${name}</a>
	</message>
	
	<message id="no-beer-search-results">
		<h3>${tr:beer-search.no-results}</h3>
	</message>
	
	<message id="bar-search-form">
	<div class="center">${header}
		
		
		<span>${tr:bar-search.select-bar}:</span>
		<div class="input-append">${field}  ${find}</div>
		${results}
		${add-bar}
		</div>
		
	</message>
	
	<message id="bar-search-results">
		<span>${tr:bar-search.results}:</span>
		<div>${result-entries}</div>
	</message>
	
	<message id="bar-search-result">
		<a href="${app.base.url}/${relative-url}">${name}</a>
	</message>
	
	<message id="no-bar-search-results">
		<span>${tr:bar-search.no-results}</span>
	</message>
	
	<message id="logged-in-user">
		<p class="navbar-text pull-right">
              ${tr:logged-in-user.logged-in-as} <a href="#" class="navbar-link">${first-name} ${last-name}</a>
        </p>
	</message>
	
	<message id="nearby-bars-form">
			<div class="header">
				${tr:nearby-bars-form.title}
			</div>
				
			<div>${tr:nearby-bars-form.location}: ${location}</div>
			<div>${tr:nearby-bars-form.radius}: ${radius} </div>
			<div>${tr:nearby-bars-form.beer}: ${beer} </div>
			<div>${search}</div>
			<table>
			<tr>
			<td>${map}</td><td>${results}</td>
			</tr>
			</table>
	</message>
	
	<message id="bar-result">
		<div>${bar}</div>
		<div>${tr:bar-form.website}: ${website}</div>
		<div>${tr:bar-form.address}: ${address1}</div>
		<div> ${address2}</div>
		<div> ${address3}</div>
		
		
	</message>
	
	
	
	
</messages>